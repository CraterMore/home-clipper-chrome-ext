{
  "manifest_version": 3,
  "name": "Home Clipper",
  "version": "0.1",
  "description": "Save homes and apartments to a database",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}